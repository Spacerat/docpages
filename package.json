{
  "name": "docpages",
  "version": "1.3.1",
  "description": "A simple tool for writing docs directly in code",
  "files": [
    "!lib/__tests__/**/*",
    "lib/**/*",
    "bin/**/*"
  ],
  "main":  "./lib/cli.js",
  "bin":  "./lib/cli.js",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf ./lib/",
    "lint": "eslint ./src/ --fix",
    "start": "TS_NODE_FILES=true ts-node ./src/cli.ts",
    "test:watch": "jest --watch",
    "test": "jest --passWithNoTests",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spacerat/docpages.git"
  },
  "license": "MIT",
  "author": {
    "name": "Joseph Atkins-Turkish",
    "email": "spacerat@users.noreply.github.com",
    "url": "https://github.com/spacerat"
  },
  "engines": {
    "node": ">=12.0"
  },
  "keywords": [
    "cli",
    "typescript",
    "documentation",
    "mustache"
  ],
  "bugs": {
    "url": "https://github.com/spacerat/docpages/issues"
  },
  "homepage": "https://github.com/spacerat/docpages#readme",
  "dependencies": {
    "commander": "^12.1.0",
    "glob-gitignore": "^1.0.15",
    "mustache": "^4.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@tsconfig/node18": "^18.2.4",
    "@types/jest": "^29.5.14",
    "@types/mustache": "^4.2.5",
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.4.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.16.0"
  }
}
